{
  "name": "wayfarer_api",
  "version": "1.0.0",
  "description": "a public transport booking server platform.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require @babel/register ./test/**/*.js",
    "lint": "eslint",
    "build": "babel src -d dist",
    "dev": "nodemon --exec babel-node src/server.js",
    "start": "node dist/server.js",
    "migrate": "node dist/migrate.js",
    "seeder": "node dist/seeder.js",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastjoe/WayFarer_Api.git"
  },
  "keywords": [
    "transport",
    "booking",
    "server"
  ],
  "author": "Nkwonta Joseph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mastjoe/WayFarer_Api/issues"
  },
  "homepage": "https://github.com/mastjoe/WayFarer_Api#readme",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "swagger-jsdoc": "^3.3.0",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "request": "^2.88.0",
    "sinon": "^7.3.2"
  }
}
